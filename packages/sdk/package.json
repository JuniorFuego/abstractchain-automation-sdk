{
  "name": "@abstractchain/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for AbstractChain Automation with ERC-4337 Account Abstraction",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./accounts": {
      "import": "./dist/modules/accounts/index.mjs",
      "require": "./dist/modules/accounts/index.js",
      "types": "./dist/modules/accounts/index.d.ts"
    },
    "./paymaster": {
      "import": "./dist/modules/paymaster/index.mjs",
      "require": "./dist/modules/paymaster/index.js",
      "types": "./dist/modules/paymaster/index.d.ts"
    },
    "./nft": {
      "import": "./dist/modules/nft/index.mjs",
      "require": "./dist/modules/nft/index.js",
      "types": "./dist/modules/nft/index.d.ts"
    },
    "./tipping": {
      "import": "./dist/modules/tipping/index.mjs",
      "require": "./dist/modules/tipping/index.js",
      "types": "./dist/modules/tipping/index.d.ts"
    },
    "./automation": {
      "import": "./dist/modules/automation/index.mjs",
      "require": "./dist/modules/automation/index.js",
      "types": "./dist/modules/automation/index.d.ts"
    },
    "./abstract": {
      "import": "./dist/modules/abstract/index.mjs",
      "require": "./dist/modules/abstract/index.js",
      "types": "./dist/modules/abstract/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ethers": "^6.7.1",
    "viem": "^1.10.0",
    "@noble/hashes": "^1.3.1",
    "@noble/secp256k1": "^2.0.0",
    "abitype": "^0.9.8"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@vitest/coverage-v8": "^0.34.3",
    "@vitest/ui": "^0.34.3",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.3"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "keywords": [
    "abstractchain",
    "erc-4337",
    "account-abstraction",
    "gasless",
    "ethereum",
    "web3",
    "defi",
    "socialfi"
  ],
  "author": "AbstractChain Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/abstractchain/automation-sdk.git",
    "directory": "packages/sdk"
  }
}